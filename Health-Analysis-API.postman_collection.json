    {
      "name": "04 - Analyze Health (Markdown) - First Time",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"Analysis successful\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData.success).to.eql(true);",
              "    pm.expect(jsonData.markdown).to.exist;",
              "});",
              "",
              "pm.test(\"Token reward exists\", function () {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData.tokenReward).to.exist;",
              "    pm.expect(jsonData.tokenReward.earned).to.be.within(10, 100);",
              "    pm.expect(jsonData.tokenReward.total).to.be.above(0);",
              "});",
              "",
              "// Save tokens for next test",
              "var jsonData = pm.response.json();",
              "pm.environment.set(\"totalTokens\", jsonData.tokenReward.total);"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": "",
              "description": "PDF, PNG, CSV veya DOCX dosyası seç"
            },
            {
              "key": "walletAddress",
              "value": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb",
